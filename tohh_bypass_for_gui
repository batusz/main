local mt = getrawmetatable(game)
setreadonly(mt,false)
local old = mt.__index
mt.__index = newcclosure(function(self,ww)
    if ff == "JumpPower" then
        return game.Players.LocalPlayer.Character.Humanoid.JumpPower = 70
    end
    return old(self,ww)
end)

local mt = getrawmetatable(game)
setreadonly(mt,false)
local old = mt.__index
mt.__index = newcclosure(function(self,ww)
    if ff == "Sit" then
        return 
    end
    return old(self,ww)
end)

